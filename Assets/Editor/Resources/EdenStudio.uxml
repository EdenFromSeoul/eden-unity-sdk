            </ui:Button>
            <ui:Button parse-escape-sequences="true" display-tooltip-when-elided="true" name="setup" class="tab-button">
                <ui:Label text="설정" name="settings_label" />
            </ui:Button>
            <ui:EnumField label="언어" type="Editor.Scripts.Manager.Language, EdenStudio.Editor" name="language_field" />
        </ui:VisualElement>
        <ui:VisualElement name="container" class="p-4 grow" />
    </ui:VisualElement>
</ui:UXML>
